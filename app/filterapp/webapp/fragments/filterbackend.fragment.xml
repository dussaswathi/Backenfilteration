        id="idSelectDialog" 
        title="OrderID'S" 
        items="{valuehelpmodel>/results}" 
        liveChange="_handleLiveChange"
        confirm="onValueHelpDialogConfirm" 
        cancel="_handleValueHelpClose" 
        afterOpen="onDialogAfterOpen">
        
